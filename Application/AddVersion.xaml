<Window x:Class="Application.AddVersion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Application"
        mc:Ignorable="d"
        Title="AddVersion" Height="450" Width="800">
    <Grid Background="#FFC2EAFF">
        <Label Content="Add Version" HorizontalAlignment="Left" Margin="10,22,0,0" VerticalAlignment="Top" Height="58" Width="210" FontSize="36" FontFamily="Tahoma"/>
        <Button Content="Exit" HorizontalAlignment="Left" Margin="669,357,0,0" VerticalAlignment="Top" Height="67" Width="121" FontSize="36" BorderBrush="Black" Background="#FFCC0000" FontWeight="Bold" Foreground="Black" Click="exit"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="669,285,0,0" VerticalAlignment="Top" Height="67" Width="121" FontSize="36" BorderBrush="Black" Background="#FF1A5CFF" FontWeight="Bold" Foreground="White" Click="back"/>
        <Label Content="Version" HorizontalAlignment="Left" Margin="10,102,0,0" VerticalAlignment="Top" Height="43" Width="94" FontSize="22"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top" Height="43" Width="120" FontSize="22"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="10,198,0,0" VerticalAlignment="Top" Height="43" Width="120" FontSize="22"/>
        <TextBox x:Name="v" HorizontalAlignment="Left" Margin="241,110,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Height="26"/>
        <TextBox x:Name="un" HorizontalAlignment="Left" Margin="241,158,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Height="26"/>
        <PasswordBox x:Name="pw" HorizontalAlignment="Left" Margin="241,206,0,0" VerticalAlignment="Top" Width="120" Height="26"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="10,357,0,0" VerticalAlignment="Top" Height="67" Width="121" FontSize="36" BorderBrush="Black" Background="#FF1A5CFF" FontWeight="Bold" Foreground="White" Click="add"/>
        <Label Content="Password again" HorizontalAlignment="Left" Margin="10,246,0,0" VerticalAlignment="Top" Height="44" Width="172" FontSize="22" RenderTransformOrigin="-1.825,-0.548"/>
        <PasswordBox x:Name="pwagain" HorizontalAlignment="Left" Margin="241,255,0,0" VerticalAlignment="Top" Width="120" Height="26"/>

    </Grid>
</Window>
